    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.18.0",
    "tailwindcss": "^4.0.15",
    "tw-animate-css": "^1.3.8",
